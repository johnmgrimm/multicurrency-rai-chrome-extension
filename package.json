  "repository": {
    "type": "git",
    "url": "https://github.com/johnmgrimm/multicurrency-rai-chrome-extension.git"
  },
  "scripts": {
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "prettier": "prettier --write '**/*.{js,jsx,css,html}'",
    "test": "jest src",
    "test:watch": "jest --watch src",
    "zip": "zip chrome-web-store/build/multicurrency-rai-chrome-extension.zip build/*"
